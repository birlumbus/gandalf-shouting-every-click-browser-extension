{
  "manifest_version": 3,
  "name": "Gandalf is grumpy",
  "version": "1.0",
  "description": "He will probably start shouting",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
